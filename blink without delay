const int ledPin = LED_BUILTIN;  // the number of the LED pin 
int ledState = LOW;  // ledState used to set the LED 

unsigned long previousMillis = 0;  // will store last time LED was updated 
const long interval = 1000;  // interval at which to blink (milliseconds) 
int pushButton = 12; 

void setup() { 
Serial.begin(9600); 

  // set the digital pin as output: 
  pinMode(11, OUTPUT); 
  pinMode(pushButton, INPUT); //set the push button as an input
} 

void loop() { 
  int buttonState = digitalRead(pushButton);  // read the push button's value

     Serial.print("buttonState = ");  // display "buttonstate =" on the serial monitor
  Serial.println(buttonState);  // display the buttonstate value on the serial monitor
  unsigned long currentMillis = millis(); // store the current time in milliseconds

  if (currentMillis - previousMillis >= interval) { 
    // save the last time you blinked the LED 
    previousMillis = currentMillis; 

    // if the LED is off turn it on and vice-versa: 
    if (ledState == LOW) { 
      ledState = HIGH; 
    } else { 
      ledState = LOW; 
    } 

    // set the LED with the ledState of the variable: 
    digitalWrite(11, ledState); 
  } 
